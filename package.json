{
  "name": "chrys",
  "version": "2.0.27",
  "description": "A collection of color palettes for mapping and visualisation",
  "main": "index.js",
  "repository": {
    "type": "git",
    "url": "git://github.com/netbek/chrys"
  },
  "files": [
    "css",
    "data",
    "illustrator",
    "src",
    "utils"
  ],
  "scripts": {
    "build-data": "node -r esm scripts/bokeh-palettes.js && node -r esm scripts/vega-palettes.js",
    "build-dist": "gulp && node -r esm scripts/build-dist.js && rm -fr build && rm -fr dist && python2.7 setup.py sdist bdist_wheel && twine check dist/*",
    "publish": "twine upload --verbose dist/*",
    "test": "make test"
  },
